{
	"manifest_version": 2,
	"name": "rawgitkit",
	"version": "1.0",
	"description": "该插件使用rawgit技术查看github项目中的html页面",
	"author": "dragonwong",
	"homepage_url": "https://github.com/dragonwong/rawgitkit",

	"permissions": [
		"tabs"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "icon.png"
	}
}